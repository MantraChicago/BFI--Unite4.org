- has_contributed_class= ((current_user) && (current_user.has_contributed_to_need need))? 'contributed' : '' 
a.btn.open-fulfillment-modal class="#{need_type_properties(need.type_of_need)[:color]}" class="#{has_contributed_class}" data-need-id=need.id data-type-of-need=need.type_of_need #{need.type_of_need.humanize}