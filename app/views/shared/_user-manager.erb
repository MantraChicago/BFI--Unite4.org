<%= if user_signed_in? and current_user
        logoutLink=link_to 'Logout', destroy_user_session_path, :method=>'delete' 
        profileLink=link_to 'Profile', profiles_path
        "Hi, #{ current_user.display_name } | #{profileLink} | #{logoutLink}".html_safe
    else
        createLink= link_to 'CREATE USER ACCOUNT', new_user_registration_path
        loginLink=link_to 'LOGIN', new_user_session_path
        createLink+' | '+loginLink
    end
%>

